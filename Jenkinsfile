pipeline {
    agent any
    
    stages {
        stage ("test") {
            steps {
                script {
                    echo "testing .net app"
                }
            }
        }
        
        stage ("build") {
            steps {
                script {
                    echo "building .net app"
                }
            }
        }
        
        stage ("deploy") {
            steps {
                script {
                    echo "deploying the .net app"
                }
            }
        }
    }
}